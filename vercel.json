{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://api.travel-vietnamese.me/$1"
    }
  ]
}
